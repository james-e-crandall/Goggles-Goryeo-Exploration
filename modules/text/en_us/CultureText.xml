		<!-- HOJOK -->
		<Row Tag="LOC_CIVIC_EX_GORYEO_HOJOK_NAME">
			<Text>Hojok</Text>
		</Row>
		<Row Tag="LOC_CIVIC_EX_GORYEO_HOJOK_QUOTE">
			<Text>All that we are is the result of what we have thought</Text>
		</Row>
		<Row Tag="LOC_CIVIC_EX_GORYEO_HOJOK_QUOTE_AUTHOR">
			<Text>Dhammapada 1.1 </Text>
		</Row>
		<Row Tag="LOC_HOJOK_MOD_GOLD_TOWNS_DESCRIPTION">
			<Text>Increased Gold from Towns. </Text>
		</Row>
		<Row Tag="LOC_HOJOK_MOD_HAPPINESS_TOWNS_DESCRIPTION">
			<Text>Decreased Happiness from Towns.</Text>
		</Row>
		<Row Tag="LOC_HOJOK_MOD_ADJUST_SETTLEMENT_CAP_DESCRIPTION">
			<Text>Increased Settlement Limit..</Text>
		</Row>

		<!-- Tradtion -->

		<!-- PATRIOTIC_WAR -->
		<Row Tag="LOC_CIVIC_EX_GORYEO_PATRIOTIC_WAR_NAME">
			<Text>Patriotic War</Text>
		</Row>
		<Row Tag="LOC_CIVIC_EX_GORYEO_PATRIOTIC_WAR_QUOTE">
			<Text>Be a lamp unto yourself. Work out your liberation with diligence.</Text>
		</Row>
		<Row Tag="LOC_CIVIC_EX_GORYEO_PATRIOTIC_WAR_QUOTE_AUTHOR">
			<Text>Mahaparinibbana Sutta</Text>
		</Row>

		<Row Tag="LOC_PATRIOTIC_WAR_MOD_DEFEND_ABILITY_DESCRIPTION">
			<Text>+3 Combat Strength on defense on your territory.</Text>
		</Row>

		<!-- Tradtion -->
